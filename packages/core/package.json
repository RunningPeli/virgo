{
  "name": "@runningpeli/virgo",
  "type": "module",
  "version": "0.0.0",
  "description": "Offering a collection of reusable components and a unique control for enhanced customization.",
  "author": "RunningPeli",
  "license": "MIT",
  "homepage": "https://virgo-ui.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RunningPeli/virgo.git"
  },
  "bugs": {
    "url": "https://github.com/RunningPeli/virgo/issues"
  },
  "keywords": [
    "virgo",
    "virgo-ui",
    "unstyled",
    "accessible",
    "headless",
    "runningpeli",
    "nuxt-module",
    "nuxt",
    "nuxt3",
    "vue",
    "vue3",
    "ui",
    "components",
    "ui-library",
    "customizable",
    "design-system",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "typecheck": "vue-tsc -p tsconfig.app.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepack": "pnpm run build",
    "release:publish": "pnpm publish --access public --no-git-checks"
  },
  "peerDependencies": {
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@nuxt/kit": "3.16.0",
    "@nuxt/schema": "3.16.0",
    "@testing-library/vue": "8.1.0",
    "@types/node": "22.13.10",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/tsconfig": "0.7.0",
    "defu": "^6.1.4",
    "jsdom": "26.0.0",
    "type-fest": "^4.37.0",
    "typescript": "5.8.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "6.2.1",
    "vite-plugin-dts": "4.5.3",
    "vitest": "3.0.8",
    "vue": "3.5.13",
    "vue-tsc": "2.2.8"
  }
}

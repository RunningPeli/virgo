    "@vue/tsconfig": "0.7.0",
    "jsdom": "26.0.0",
    "typescript": "5.8.2",
    "vite": "6.2.1",
    "vite-plugin-dts": "4.5.3",
    "vitest": "3.0.8",
    "vue": "3.5.13",
    "vue-tsc": "2.2.8"
  }

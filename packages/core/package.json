{
  "name": "@runningpeli/virgo",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
